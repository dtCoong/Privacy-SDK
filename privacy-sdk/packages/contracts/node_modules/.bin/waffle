#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Privacy-SDK/privacy-sdk/node_modules/.pnpm/ethereum-waffle@4.0.10_@ens_429e82a7efa981760f243f45851c75d9/node_modules/ethereum-waffle/bin/node_modules:/mnt/d/Privacy-SDK/privacy-sdk/node_modules/.pnpm/ethereum-waffle@4.0.10_@ens_429e82a7efa981760f243f45851c75d9/node_modules/ethereum-waffle/node_modules:/mnt/d/Privacy-SDK/privacy-sdk/node_modules/.pnpm/ethereum-waffle@4.0.10_@ens_429e82a7efa981760f243f45851c75d9/node_modules:/mnt/d/Privacy-SDK/privacy-sdk/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Privacy-SDK/privacy-sdk/node_modules/.pnpm/ethereum-waffle@4.0.10_@ens_429e82a7efa981760f243f45851c75d9/node_modules/ethereum-waffle/bin/node_modules:/mnt/d/Privacy-SDK/privacy-sdk/node_modules/.pnpm/ethereum-waffle@4.0.10_@ens_429e82a7efa981760f243f45851c75d9/node_modules/ethereum-waffle/node_modules:/mnt/d/Privacy-SDK/privacy-sdk/node_modules/.pnpm/ethereum-waffle@4.0.10_@ens_429e82a7efa981760f243f45851c75d9/node_modules:/mnt/d/Privacy-SDK/privacy-sdk/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ethereum-waffle/bin/waffle" "$@"
else
  exec node  "$basedir/../ethereum-waffle/bin/waffle" "$@"
fi
