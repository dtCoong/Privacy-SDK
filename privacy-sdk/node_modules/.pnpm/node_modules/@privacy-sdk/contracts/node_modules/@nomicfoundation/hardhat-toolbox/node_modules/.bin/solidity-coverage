#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Privacy-SDK/privacy-sdk/node_modules/.pnpm/solidity-coverage@0.8.16_ha_71ef51dd26107fa16522d509d53f8e33/node_modules/solidity-coverage/plugins/node_modules:/mnt/d/Privacy-SDK/privacy-sdk/node_modules/.pnpm/solidity-coverage@0.8.16_ha_71ef51dd26107fa16522d509d53f8e33/node_modules/solidity-coverage/node_modules:/mnt/d/Privacy-SDK/privacy-sdk/node_modules/.pnpm/solidity-coverage@0.8.16_ha_71ef51dd26107fa16522d509d53f8e33/node_modules:/mnt/d/Privacy-SDK/privacy-sdk/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Privacy-SDK/privacy-sdk/node_modules/.pnpm/solidity-coverage@0.8.16_ha_71ef51dd26107fa16522d509d53f8e33/node_modules/solidity-coverage/plugins/node_modules:/mnt/d/Privacy-SDK/privacy-sdk/node_modules/.pnpm/solidity-coverage@0.8.16_ha_71ef51dd26107fa16522d509d53f8e33/node_modules/solidity-coverage/node_modules:/mnt/d/Privacy-SDK/privacy-sdk/node_modules/.pnpm/solidity-coverage@0.8.16_ha_71ef51dd26107fa16522d509d53f8e33/node_modules:/mnt/d/Privacy-SDK/privacy-sdk/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../solidity-coverage/plugins/bin.js" "$@"
else
  exec node  "$basedir/../../../../solidity-coverage/plugins/bin.js" "$@"
fi
