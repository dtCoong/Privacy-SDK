#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/privacy-sdk/node_modules/.pnpm/hardhat@2.22.0_ts-node@10.9_9f1676be1434e44ba1ae4ec2ed6b4bd2/node_modules/hardhat/internal/cli/node_modules:/mnt/d/privacy-sdk/node_modules/.pnpm/hardhat@2.22.0_ts-node@10.9_9f1676be1434e44ba1ae4ec2ed6b4bd2/node_modules/hardhat/internal/node_modules:/mnt/d/privacy-sdk/node_modules/.pnpm/hardhat@2.22.0_ts-node@10.9_9f1676be1434e44ba1ae4ec2ed6b4bd2/node_modules/hardhat/node_modules:/mnt/d/privacy-sdk/node_modules/.pnpm/hardhat@2.22.0_ts-node@10.9_9f1676be1434e44ba1ae4ec2ed6b4bd2/node_modules:/mnt/d/privacy-sdk/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/privacy-sdk/node_modules/.pnpm/hardhat@2.22.0_ts-node@10.9_9f1676be1434e44ba1ae4ec2ed6b4bd2/node_modules/hardhat/internal/cli/node_modules:/mnt/d/privacy-sdk/node_modules/.pnpm/hardhat@2.22.0_ts-node@10.9_9f1676be1434e44ba1ae4ec2ed6b4bd2/node_modules/hardhat/internal/node_modules:/mnt/d/privacy-sdk/node_modules/.pnpm/hardhat@2.22.0_ts-node@10.9_9f1676be1434e44ba1ae4ec2ed6b4bd2/node_modules/hardhat/node_modules:/mnt/d/privacy-sdk/node_modules/.pnpm/hardhat@2.22.0_ts-node@10.9_9f1676be1434e44ba1ae4ec2ed6b4bd2/node_modules:/mnt/d/privacy-sdk/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../hardhat/internal/cli/bootstrap.js" "$@"
else
  exec node  "$basedir/../hardhat/internal/cli/bootstrap.js" "$@"
fi
